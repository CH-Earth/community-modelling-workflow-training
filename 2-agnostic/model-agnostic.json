{
  "exec": {
    "met": "$HOME/github-repos/datatool/extract-dataset.sh",
    "gis": "$HOME/github-repos/gistool/extract-gis.sh",
    "remap": "easymore cli"
  },

  "args": {
    "met": [{
      "dataset": "CaSRv3.2",
      "dataset-dir": "/project/rrg-alpie/data/meteorological-data/casrv3.2/",
      "variable": [
        "CaSR_v3.2_P_P0_SFC",
        "CaSR_v3.2_P_TT_09975",
        "CaSR_v3.2_P_UVC_09975",
        "CaSR_v3.2_A_PR0_SFC",
        "CaSR_v3.2_P_FB_SFC",
        "CaSR_v3.2_P_FI_SFC",
        "CaSR_v3.2_P_HU_09975"
      ],
      "output-dir": "$HOME/scratch/bc-models/datatool-outputs",
      "start-date": "1980-01-01T13:00:00",
      "end-date": "1980-01-5T12:00:00",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "$HOME/github-repos/maf/1-geofabric/bow-at-calgary-geofabric/bc_subbasins.shp",
      "model": "",
      "cache": "$SLURM_TMPDIR",
      "ensemble": "",
      "prefix": "bc_model_",
      "email": "",
      "cluster": "./etc/clusters/drac.json",
      "_flags": [
        "submit-job",
        "parsable"
      ]
    }],

    "gis": [{
      "dataset": "landsat",
      "dataset-dir": "/project/rrg-alpie/data/geospatial-data/Landsat",
      "variable": "land-cover",
      "start-date": "2015",
      "end-date": "2015",
      "output-dir": "$HOME/scratch/bc-models/gistool-outputs",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "$HOME/github-repos/maf/1-geofabric/bow-at-calgary-geofabric/bc_subbasins.shp",
      "print-geotiff": "true",
      "stat": [
        "frac",
        "coords"
      ],
      "quantile": "",
      "cache": "$SLURM_TMPDIR",
      "prefix": "bc_model_",
      "email": "",
      "fid": "COMID",
      "cluster": "./etc/clusters/ucalgary-arc.json",
      "_flags": [
	"include-na",
        "submit-job",
        "parsable"
      ]
    },

    {
      "dataset": "soil_class",
      "dataset-dir": "/project/rrg-alpie/data/geospatial-data/soil_classes",
      "variable": "soil_classes",
      "start-date": "",
      "end-date": "",
      "output-dir": "$HOME/scratch/bc-models/gistool-outputs",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "$HOME/github-repos/maf/1-geofabric/bow-at-calgary-geofabric/bc_subbasins.shp",
      "print-geotiff": "true",
      "stat": [
        "majority"
      ],
      "quantile": "",
      "cache": "$SLURM_TMPDIR",
      "prefix": "bc_model_",
      "email": "",
      "fid": "COMID",
      "cluster": "./etc/clusters/ucalgary-arc.json",
      "_flags": [
        "include-na",
        "submit-job",
        "parsable"
      ]
    }],

    "remap": [{
      "case-name": "remapped",
      "cache": "$HOME/scratch/bc-models/easymore-outputs/cache/",
      "shapefile": "$HOME/github-repos/maf/1-geofabric/bow-at-calgary-geofabric/bc_subbasins.shp",
      "shapefile-id": "COMID",
      "source-nc": "$HOME/scratch/bc-models/datatool-outputs/**/*.nc*",
      "variable-lon": "lon",
      "variable-lat": "lat",
      "variable": [
        "CaSR_v3.2_P_P0_SFC",
        "CaSR_v3.2_P_TT_09975",
        "CaSR_v3.2_P_UVC_09975",
        "CaSR_v3.2_A_PR0_SFC",
        "CaSR_v3.2_P_FB_SFC",
        "CaSR_v3.2_P_FI_SFC",
        "CaSR_v3.2_P_HU_09975"
      ],
      "remapped-var-id": "COMID",
      "remapped-dim-id": "COMID",
      "output-dir": "$HOME/scratch/bc-models/easymore-outputs/",
      "job-conf": "$HOME/github-repos/maf/2-agnostic/easymore-job.slurm",
      "_flags": [
        "submit-job"
      ]
    }]
  },
  "order": {
    "met": 1,
    "gis": -1,
    "remap": 2
  }
}
